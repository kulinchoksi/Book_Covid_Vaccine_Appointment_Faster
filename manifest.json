{
	"manifest_version" : 2,
	"name" : "Book Covid Vaccine Appointment Faster",
	"description" : "This extension automize few steps to book appointment on CoWIN website for 18+ age",
	"version" : "1.0",
	"content_scripts" : [{
			"matches" : [
				"https://selfregistration.cowin.gov.in/appointment"
			],
			"js" : ["popup.js"],
			"run_at" : "document_end"
		}
	],
	"browser_action" : {
		"default_icon" : "covid_vaccine.jpg",
		"default_popup" : "popup.html"
	},
	"permissions" : [
		"activeTab"
	]
}
